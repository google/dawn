#include <metal_stdlib>

using namespace metal;
void compute_main_inner(uint local_invocation_index, threadgroup atomic_int* const tint_symbol) {
  {
    atomic_store_explicit(tint_symbol, 0, memory_order_relaxed);
  }
  threadgroup_barrier(mem_flags::mem_threadgroup);
  int res = atomic_fetch_sub_explicit(tint_symbol, -1, memory_order_relaxed);
}

kernel void compute_main(uint local_invocation_index [[thread_index_in_threadgroup]]) {
  threadgroup atomic_int tint_symbol_1;
  compute_main_inner(local_invocation_index, &(tint_symbol_1));
  return;
}

