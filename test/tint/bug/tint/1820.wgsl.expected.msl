#include <metal_stdlib>
using namespace metal;

struct tint_module_vars_struct {
  thread int* global;
};

int tint_f32_to_i32(float value) {
  return int(clamp(value, -2147483648.0f, 2147483520.0f));
}

void foo(float x) {
  switch(tint_f32_to_i32(x)) {
    default:
    {
      break;
    }
  }
}

int baz(int x, tint_module_vars_struct tint_module_vars) {
  (*tint_module_vars.global) = 42;
  return x;
}

void bar(float x, tint_module_vars_struct tint_module_vars) {
  switch(baz(tint_f32_to_i32(x), tint_module_vars)) {
    default:
    {
      break;
    }
  }
}

[[max_total_threads_per_threadgroup(1)]]
kernel void v() {
  thread int global = 0;
  tint_module_vars_struct const tint_module_vars = tint_module_vars_struct{.global=(&global)};
  foo(1.0f);
  bar(1.0f, tint_module_vars);
}
